<%= simple_form_for @room do |f| %>
  <%= f.input :type_id, collection: @categories, prompt: "Select a catagory" %>
  <%= f.input :address, label: "Address" %>
  <%= f.input :description, label: "Property Description" %>
  <%= f.input :price, label: "Price" %>
  <%= f.input :contact_info, label: "Contact Information" %>
  <%= f.button :submit %>
<% end %>
