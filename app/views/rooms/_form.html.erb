<!-- contains form that users submit to post rooms -->
<%= simple_form_for @room, :html => {:multipart => true } do |f| %>
  <%= select_tag(:type_id, options_for_select(@types), :prompt => "Choose the room type") %>
  <%= f.input :address, label: "Address" %>
  <%= f.input :description, label: "Property Description" %>
  <%= f.input :price, label: "Price" %>
  <%= f.input :contact_info, label: "Contact Information" %>
  <%= f.button :submit %>
<% end %>
